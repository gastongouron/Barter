<br>
<div class="panel-console">
        <% if current_user && current_user.timecoin >= 1 %>
        <%= link_to "New Service", new_service_path, class: "new pull-right" %>
        <% end %>

<p>Hello</p>
</div>

<div id="services">
  <% @activeservices.each do |service| %>
    <%= link_to service do %>
    <div class="panel-service-active">
      <% prop = User.find_by(id: service.servicer_id) %>
<%= image_tag(prop.avatar.url(:medium), class: 'round-image-50') %>&nbsp;&nbsp;


    <span class="h3 main-color"><%= service.name %></span><br><br>
    <span>Created by <b><%= service.servicer_name %></b></span>
    <span>at <b> <%= service.created_at %></b></span><br>
    <span>Starts <b> <%= service.start %></b></span>
    <span>Ends <b> <%= service.end %></b></span><br>

    <span>description<b> <%= service.description %></b></span>

    </div><br>
    <% end %>
  <% end %>
</div>


<div id="services">
  <% @archivedservices.each do |service| %>
    <div class="panel-service-active">
    <p><h2><%= link_to service.name, service %> created by <b><%= service.servicer_name %></b></h2></p>
    <p><b>created</b> <%= service.created_at %><br>
    <b>description</b> <%= service.description %></p>
    </div><br>
  <% end %>
</div>
